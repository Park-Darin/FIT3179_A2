  "layer": [
    {
      "params": [
        {
          "name": "hover_selection",
          "select": {
            "type": "point",
            "fields": ["Type of Protein"],
            "on": "mouseover",
            "clear": "mouseout"
          }
        }
      ],
      "mark": {"type": "bar"},
      "encoding": {
        "x": {
          "field": "Average (%)",
          "type": "quantitative",
          "title": "Average (%)"
        },
        "y": {
          "field": "Entity",
          "type": "nominal",
          "title": "Entity",
          "sort": ["Malaysia", "World", "Australia"]
        },
        "color": {
          "field": "Type of Protein",
          "type": "nominal",
          "title": "Type of Protein",
          "scale": {
            "domain": [
